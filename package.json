{
  "name": "voiceover-studio-finder",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Modern platform for finding and connecting voiceover professionals with recording studios",
  "overrides": {
    "tmp": "^0.2.4",
    "tough-cookie": "^4.1.3"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:full": "bash scripts/dev-full.sh",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:fast": "oxlint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "deps:check": "depcheck",
    "build:analyze": "ANALYZE=true npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:studio": "prisma studio",
    "db:check": "bash scripts/show-db-env.sh",
    "db:sync": "bash scripts/sync-prod-to-dev.sh",
    "db:sync-prod-to-dev": "tsx scripts/sync-production-to-dev.ts",
    "db:sync-tool": "tsx scripts/db-sync-tool.ts",
    "db:sync-faq-waitlist": "tsx scripts/sync-faq-waitlist-bidirectional.ts --confirm",
    "db:migrate:dev": "bash scripts/migrate-with-protection.sh dev",
    "db:migrate:prod": "bash scripts/migrate-with-protection.sh production",
    "migrate:studio-types": "tsx scripts/migrate-studio-types.ts",
    "migrate:studio-types:dry": "tsx scripts/migrate-studio-types.ts --dry-run",
    "privacy:disable-visibility": "tsx scripts/disable-all-visibility-settings.ts",
    "privacy:disable-visibility:dry": "tsx scripts/disable-all-visibility-settings.ts --dry-run",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "clean": "rm -rf .next node_modules",
    "deploy:build": "npm run db:generate && npm run build",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "docker-compose build && docker-compose up -d",
    "deploy:check": "npm run type-check && npm run lint && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:links": "broken-link-checker http://localhost:3000 --recursive --follow --ordered --verbose --exclude=\"twitter.com\"",
    "test:links:local": "broken-link-checker http://localhost:3000 --recursive --follow --ordered --verbose",
    "test:lighthouse": "lhci autorun",
    "audit:quick": "npm run lint:fast && npm run deps:check",
    "audit:full": "npm run type-check && npm run lint && npm run lint:fast && npm run deps:check",
    "audit:all": "npm run lint && npm run lint:fast && npm run deps:check && npm run build:analyze && npm run test:links && npm run test:lighthouse",
    "cleanup:neon-branches": "tsx scripts/cleanup-neon-branches.ts",
    "email:diagnose": "tsx scripts/diagnose-email-config.ts",
    "dummy-data:add": "tsx scripts/add-dummy-admin-data.ts",
    "dummy-data:delete": "tsx scripts/delete-dummy-admin-data.ts",
    "stripe:setup": "bash scripts/setup-stripe-dev.sh",
    "stripe:setup:dev": "bash scripts/setup-stripe-dev.sh --start-dev",
    "stripe:stop": "bash scripts/stop-stripe-dev.sh",
    "stripe:stop:clean": "bash scripts/stop-stripe-dev.sh --clean",
    "user:delete": "tsx scripts/delete-user-data.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.16.3",
    "@sentry/nextjs": "^10.32.1",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.0",
    "@types/archiver": "^7.0.0",
    "@vercel/analytics": "^1.5.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.25.0",
    "ioredis": "^5.8.1",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "next": "^16.1.1",
    "next-auth": "^4.24.11",
    "prisma": "^6.16.3",
    "react": "^19.2.3",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.2.3",
    "react-easy-crop": "^5.5.6",
    "react-hook-form": "^7.69.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@jest/globals": "^30.2.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.40.0",
    "@swc/core": "^1.15.7",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "broken-link-checker": "^0.7.8",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-fetch": "^2.7.0",
    "oxlint": "^1.35.0",
    "playwright": "^1.40.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5",
    "undici": "^7.18.2"
  }
}
