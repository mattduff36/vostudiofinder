{
  "name": "voiceover-studio-finder",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Modern platform for finding and connecting voiceover professionals with recording studios",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:fast": "oxlint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "deps:check": "depcheck",
    "build:analyze": "ANALYZE=true npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:studio": "prisma studio",
    "db:check": "bash scripts/show-db-env.sh",
    "db:sync": "bash scripts/sync-prod-to-dev.sh",
    "db:migrate:dev": "bash scripts/migrate-with-protection.sh dev",
    "db:migrate:prod": "bash scripts/migrate-with-protection.sh production",
    "migrate:studio-types": "tsx scripts/migrate-studio-types.ts",
    "migrate:studio-types:dry": "tsx scripts/migrate-studio-types.ts --dry-run",
    "privacy:disable-visibility": "tsx scripts/disable-all-visibility-settings.ts",
    "privacy:disable-visibility:dry": "tsx scripts/disable-all-visibility-settings.ts --dry-run",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "clean": "rm -rf .next node_modules",
    "deploy:build": "npm run db:generate && npm run build",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "docker-compose build && docker-compose up -d",
    "deploy:check": "npm run type-check && npm run lint && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit:quick": "npm run lint:fast && npm run deps:check",
    "audit:full": "npm run type-check && npm run lint && npm run lint:fast && npm run deps:check",
    "cleanup:neon-branches": "tsx scripts/cleanup-neon-branches.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@hookform/resolvers": "^5.2.1",
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/adapter-libsql": "^6.16.3",
    "@prisma/client": "^6.16.3",
    "@sentry/nextjs": "^10.17.0",
    "@stripe/stripe-js": "^8.0.0",
    "@types/archiver": "^7.0.0",
    "@vercel/analytics": "^1.5.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.1",
    "lucide-react": "^0.554.0",
    "nanoid": "^5.1.6",
    "next": "^16.0.9",
    "next-auth": "^4.24.11",
    "prisma": "^7.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-hot-toast": "^2.6.0",
    "resend": "^6.1.2",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.40.0",
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-config-next": "^16.0.3",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-fetch": "^2.7.0",
    "oxlint": "^1.31.0",
    "playwright": "^1.40.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  }
}
